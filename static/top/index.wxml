 <view class='status-bar'>
    <view class='goBack' bindtap='goBack' style="padding-top:{{bar_Height}}px;"  hidden='{{show_bol}}'>
      <image class="img" src='./static/back.png'></image>
    </view>
    <view class='home' bindtap='home' style="padding-top:{{bar_Height}}px;"  hidden='{{home}}'>
      <image class="img" src='./static/home.png'></image>    
    </view>
    <view class="tabar {{my_class ? 'tabar2':''}}" style="padding-top:{{bar_Height}}px;">
      <text class="red">{{title}}</text>
    </view>
  </view>